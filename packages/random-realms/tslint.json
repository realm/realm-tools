{
    "extends": [
      "tslint:latest",
      "tslint-eslint-rules",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "object-literal-sort-keys": false,
      "prettier": [
        true,
        {
          "trailingComma": "all"
        }
      ]
    }
  }